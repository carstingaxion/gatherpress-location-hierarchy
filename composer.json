{
    "name": "carstingaxion/gatherpress-calendar",
    "description": "A calendar block that displays Query Loop results in a monthly calendar format. Works with any post type, with specialized support for GatherPress events.",
    "license": "GPL-3.0+",
    "type": "wordpress-plugin",
    "keywords": [
        "wordpress",
        "wordpress-plugin"
    ],
    "authors": [
        {
            "name": "figuren.theater",
            "email": "info@figuren.theater"
        },
        {
            "name": "Carsten Bach",
            "email": "mail@carsten-bach.de",
            "homepage": "https://carsten-bach.de",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/carstingaxion/gatherpress-calendar",
    "support": {
        "email": "info@figuren.theater",
        "issues": "https://github.com/carstingaxion/gatherpress-calendar/issues",
        "source": "https://github.com/carstingaxion/gatherpress-calendar"
    },
    "require-dev": {
        "figuren-theater/code-quality": "*"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true,
            "figuren-theater/altis-core": true,
            "figuren-theater/ft-core": true,
            "koodimonni/composer-dropin-installer": true,
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "altis": {
            "install-overrides": []
        }
    }
}